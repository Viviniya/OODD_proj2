<%= form_for(@realtor )do |f| %>

  <div class="field form-group">
    <%= f.label :name ,  {:class => "col-md-4 control-label" }  %>
    <%= f.text_field :name, autofocus: true, autocomplete: "name" %>
  </div>

  <div class="field">
    <%= f.label :email, {:class => "col-md-4 control-label" }   %>
    <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
  </div>

  <div class="field">
    <%= f.label :password,  {:class => "col-md-4 control-label" }   %>
    <% if @minimum_password_length %>
      <em>(<%= @minimum_password_length %> characters minimum)</em>
    <% end %>
    <%= f.password_field :password, autocomplete: "new-password" %>
  </div>

  <div class="field">
    <%= f.label :Confirm_Password,  {:class => "col-md-4 control-label" }   %>
    <%= f.password_field :password_confirmation, autocomplete: "new-password" %>
  </div>
  <div class="field">
    <%= f.label :phone_no, {:class => "col-md-4 control-label" }   %>
    <%= f.text_field :phoneNumber, autofocus: true, autocomplete: "phoneNumber" %>
  </div>
  <div class="field">
    <%= f.label :Company_Name, {:class => "col-md-4 control-label" }   %>
    <%= f.collection_select(:companyId,Company.all, :id, :name, {:include_blank => "Please select Company"}, {:class=>"col-md-4 control-label"})%>

  </div>

  <div class="actions">
    <%= f.submit "Add", {:class => "btn btn-lg btn-primary btn-block"} %>
  </div>
<% end %>